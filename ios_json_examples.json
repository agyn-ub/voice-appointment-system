    "swift_model_example": {
        "description": "Suggested Swift data models for parsing the JSON response",
        "code": "// MARK: - Response Models\nstruct AppointmentResponse: Codable {\n    let result: AppointmentResult?\n    let error: ErrorResponse?\n}\n\nstruct AppointmentResult: Codable {\n    let success: Bool\n    let message: String\n    let intent: String\n    let details: AppointmentDetails\n    let appointments: [Appointment]?\n}\n\nstruct AppointmentDetails: Codable {\n    let startDate: String?\n    let endDate: String?\n    let title: String?\n    let date: String?\n    let time: String?\n    let duration: Int?\n    let attendees: [String]?\n    \n    enum CodingKeys: String, CodingKey {\n        case startDate = \"start_date\"\n        case endDate = \"end_date\"\n        case title, date, time, duration, attendees\n    }\n}\n\nstruct Appointment: Codable, Identifiable {\n    let id: String\n    let title: String\n    let date: String\n    let time: String\n    let duration: Int\n    let attendees: [String]\n    let status: String\n    \n    // Computed properties for UI\n    var formattedDateTime: String {\n        // Convert date and time to user-friendly format\n        // Implementation depends on your date formatting needs\n    }\n    \n    var attendeesText: String {\n        attendees.isEmpty ? \"No attendees\" : attendees.joined(separator: \", \")\n    }\n}\n\nstruct ErrorResponse: Codable {\n    let message: String\n    let status: String\n}"
    }
